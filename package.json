{
  "name": "@travi/matt.travi.org-components",
  "description": "react components for my profile site",
  "version": "0.0.0-semantically-released",
  "main": "lib/components.cjs.js",
  "module": "lib/components.es.js",
  "sideEffects": false,
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi-org/matt.travi.org-components",
  "bugs": "https://github.com/travi-org/matt.travi.org-components/issues",
  "homepage": "https://github.com/travi-org/matt.travi.org-components#readme",
  "scripts": {
    "clean": "rimraf storybook-static/ lib/",
    "lint:md": "remark --quiet . --frail",
    "lint:js": "eslint . .storybook --cache",
    "test:unit:raw": "mocha --recursive test/unit",
    "test:unit": "run-s coverage",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "coverage": "nyc run-s test:unit:raw",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "start": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "snapshot": "ANY_SEED=452204917091.8763 build-storybook && percy-storybook --widths=320,1280",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "prepack": "run-s build",
    "deploy": "gh-pages -r \"https://$GH_TOKEN@github.com/travi-org/matt.travi.org-components.git\" -d storybook-static/"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "^7.0.0",
    "@emotion/babel-preset-css-prop": "10.0.9",
    "@percy-io/percy-storybook": "2.1.0",
    "@storybook/addon-actions": "5.0.3",
    "@storybook/addon-info": "5.0.2",
    "@storybook/addon-links": "5.0.2",
    "@storybook/react": "5.0.2",
    "@travi/any": "1.8.5",
    "@travi/babel-preset": "3.0.0",
    "@travi/eslint-config-travi": "1.8.0",
    "babel-loader": "8.0.5",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "chai": "4.2.0",
    "codecov": "3.2.0",
    "commitlint-config-travi": "1.2.40",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.10.0",
    "gh-pages": "2.0.1",
    "globstar": "1.0.0",
    "husky": "1.3.1",
    "microformat-node": "2.0.1",
    "mocha": "6.0.2",
    "npm-run-all": "4.1.5",
    "nyc": "13.3.0",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-test-renderer": "16.8.4",
    "regenerator-runtime": "0.13.1",
    "remark-cli": "6.0.1",
    "remark-preset-lint-travi": "1.2.0",
    "rimraf": "2.6.3",
    "rollup": "1.6.0",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-babel": "^4.0.1",
    "rollup-plugin-json": "3.1.0",
    "rollup-plugin-node-resolve": "4.0.1"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "@emotion/core": "10.0.9",
    "@material-ui/core": "3.9.2",
    "@material-ui/icons": "3.0.2",
    "classnames": "2.2.6",
    "prop-types": "15.7.2",
    "travi": "1.0.0"
  }
}
