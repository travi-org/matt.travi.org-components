{
  "name": "@travi/matt.travi.org-components",
  "description": "react components for my profile site",
  "version": "0.0.0-semantically-released",
  "main": "lib/components.cjs.js",
  "module": "lib/components.es.js",
  "sideEffects": false,
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi-org/matt.travi.org-components",
  "bugs": "https://github.com/travi-org/matt.travi.org-components/issues",
  "homepage": "https://github.com/travi-org/matt.travi.org-components#readme",
  "scripts": {
    "clean": "rimraf storybook-static/ lib/",
    "lint:md": "remark --quiet . --frail",
    "lint:js": "eslint . .storybook --cache",
    "test:unit:raw": "mocha --recursive test/unit",
    "test:unit": "run-s coverage",
    "test:integration": "start-server-and-test 'npm start -- --quiet' http://localhost:6006 'npm run cypress:run -- ${CYPRESS_RECORD_KEY:+--record}'",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "coverage": "nyc run-s test:unit:raw",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "start": "start-storybook --port 6006 --ci",
    "build-storybook": "build-storybook --quiet",
    "presnapshot": "ANY_SEED=452204917091.8763 run-s build-storybook",
    "snapshot": "percy-storybook --widths=320,1280",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prepack": "run-s build",
    "deploy": "gh-pages -r \"https://$GH_TOKEN@github.com/travi-org/matt.travi.org-components.git\" -d storybook-static/"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "7.7.4",
    "@emotion/babel-preset-css-prop": "10.0.23",
    "@percy-io/percy-storybook": "2.1.0",
    "@storybook/addon-actions": "^5.2.7",
    "@storybook/addon-info": "^5.2.7",
    "@storybook/addon-links": "^5.2.7",
    "@storybook/react": "^5.2.7",
    "@testing-library/cypress": "^5.0.0",
    "@travi/any": "2.0.6",
    "@travi/babel-preset": "3.0.7",
    "@travi/eslint-config": "1.0.28",
    "@travi/eslint-config-cypress": "1.0.9",
    "@travi/eslint-config-mocha": "1.0.6",
    "@travi/eslint-config-react": "1.0.21",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "chai": "4.2.0",
    "codecov": "3.6.1",
    "commitlint-config-travi": "1.3.4",
    "cypress": "^3.4.0",
    "cz-conventional-changelog": "3.0.2",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "eslint-plugin-cypress": "^2.6.0",
    "gh-pages": "2.1.1",
    "globstar": "1.0.0",
    "husky": "3.1.0",
    "microformat-node": "2.0.1",
    "mocha": "6.2.2",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-test-renderer": "16.12.0",
    "remark-cli": "7.0.1",
    "remark-preset-lint-travi": "1.2.2",
    "rimraf": "3.0.0",
    "rollup": "1.27.5",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "sinon": "^7.5.0",
    "start-server-and-test": "^1.9.1"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "@emotion/core": "10.0.22",
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.2.1",
    "classnames": "2.2.6",
    "prop-types": "15.7.2",
    "travi": "1.0.0"
  }
}
