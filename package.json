{
  "name": "@travi/matt.travi.org-components",
  "description": "react components for my profile site",
  "version": "0.0.0-semantically-released",
  "main": "lib/components.cjs.js",
  "module": "lib/components.es.js",
  "sideEffects": false,
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi-org/matt.travi.org-components",
  "bugs": "https://github.com/travi-org/matt.travi.org-components/issues",
  "homepage": "https://matt-components.travi.org",
  "scripts": {
    "clean": "rimraf storybook-static/ lib/",
    "lint:md": "remark --quiet . --frail",
    "lint:js": "eslint . .storybook --cache",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "lint:peer": "npm ls >/dev/null",
    "test:unit:raw": "mocha 'src/**/*-test.js'",
    "test:unit": "run-s coverage",
    "disabled-test:integration": "start-server-and-test 'npm start -- --quiet' http://localhost:6006 'npm run cypress:run -- ${CYPRESS_RECORD_KEY:+--record}'",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "coverage": "nyc run-s test:unit:raw",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "start": "start-storybook --port 6006 --ci",
    "build:storybook": "build-storybook --quiet",
    "presnapshot": "ANY_SEED=452204917091.8763 run-s build-storybook",
    "snapshot": "percy-storybook --widths=320,1280",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prepack": "run-s build",
    "predeploy:netlify": "npm run build",
    "deploy:netlify": "npx --package netlify-cli netlify deploy --dir=./storybook-static --prod"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "7.8.6",
    "@emotion/babel-preset-css-prop": "10.0.27",
    "@percy-io/percy-storybook": "^2.1.0",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-info": "^5.3.14",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/react": "^5.3.14",
    "@testing-library/cypress": "^6.0.0",
    "@travi/any": "2.0.7",
    "@travi/babel-preset": "3.0.14",
    "@travi/eslint-config": "1.0.33",
    "@travi/eslint-config-cypress": "1.0.17",
    "@travi/eslint-config-mocha": "1.0.8",
    "@travi/eslint-config-react": "1.0.28",
    "@travi/travi.org-theme-components": "^1.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "chai": "4.2.0",
    "codecov": "3.6.5",
    "commitlint-config-travi": "1.3.6",
    "cypress": "^4.0.0",
    "cz-conventional-changelog": "3.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "globstar": "1.0.0",
    "husky": "4.2.3",
    "lockfile-lint": "^4.0.0",
    "microformat-node": "2.0.1",
    "mocha": "7.1.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-test-renderer": "16.13.1",
    "remark-cli": "7.0.1",
    "remark-preset-lint-travi": "^1.3.0",
    "rimraf": "3.0.2",
    "rollup": "2.1.0",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "sinon": "^9.0.0",
    "start-server-and-test": "^1.10.6"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "@travi/brand": "^1.0.0",
    "@travi/travi.org-core-components": "1.0.0",
    "polished": "^3.4.2",
    "prop-types": "15.7.2",
    "travi": "1.0.0"
  }
}
