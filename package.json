{
  "name": "@travi/matt.travi.org-components",
  "description": "react components for my profile site",
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi-org/matt.travi.org-components",
  "bugs": "https://github.com/travi-org/matt.travi.org-components/issues",
  "homepage": "https://github.com/travi-org/matt.travi.org-components#readme",
  "scripts": {
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint . .storybook --cache",
    "test:unit:raw": "mocha --recursive test/unit",
    "test:unit": "run-s coverage",
    "test": "run-s lint:* test:*",
    "coverage": "nyc run-s test:unit:raw",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "start": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy": "gh-pages -r \"https://$GH_TOKEN@github.com/travi-org/matt.travi.org-components.git\" -d storybook-static/",
    "greenkeeper:update-lockfile": "greenkeeper-lockfile-update",
    "greenkeeper:upload-lockfile": "greenkeeper-lockfile-upload"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "3.2.0",
    "@storybook/addon-actions": "3.3.3",
    "@storybook/addon-links": "3.2.6",
    "@storybook/react": "^3.2.4",
    "@travi/any": "1.6.0",
    "@travi/eslint-config-travi": "1.6.0",
    "babel-env": "2.4.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "codecov": "2.3.0",
    "commitlint-config-travi": "1.1.0",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "2.9.1",
    "gh-pages": "1.1.0",
    "globstar": "1.0.0",
    "greenkeeper-lockfile": "1.9.0",
    "husky": "0.14.3",
    "markdownlint-cli": "0.6.0",
    "mocha": "3.5.3",
    "npm-run-all": "4.1.2",
    "nyc": "11.4.1",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "react-test-renderer": "16.2.0"
  },
  "peerDependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "dependencies": {
    "material-ui": "1.0.0-beta.4"
  }
}
